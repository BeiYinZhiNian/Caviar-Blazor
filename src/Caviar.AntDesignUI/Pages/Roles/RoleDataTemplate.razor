@page "/ApplicationRole/CreateEntity"
@page "/ApplicationRole/UpdateEntity"
@inherits DataComponentBase<ApplicationRoleView,ApplicationRole>

<Form @ref="_meunForm" 
    LabelColSpan="6"
    WrapperColSpan="16"
    Model="@DataSource" 
    OnFinish="FormSubmit" 
    ValidateMode="@FormValidateMode.Rules" 
    ValidateOnChange="true">
    <CavFormItem  FieldName='Name' FieldRules='@context.Entity'>
        <Input DebounceMilliseconds="-1"@bind-Value='@context.Entity.Name' />
    </CavFormItem>
    <CavFormItem FieldName='DataRange' FieldRules='@context.Entity'>
        <CavEnumSelect TEnum="DataRange"  OnSelectedItemChanged="CheckMenuType" @bind-Value="context.Entity.DataRange" />
    </CavFormItem>
    <CavFormItem  FieldName='IsDisable' FieldRules='@context.Entity'>
        <Switch @bind-Value='@context.Entity.IsDisable'/>
    </CavFormItem>
    <CavFormItem  FieldName='Number' FieldRules='@context.Entity'>
        <Input DebounceMilliseconds="-1"@bind-Value='@context.Entity.Number' />
    </CavFormItem>
    <CavFormItem  FieldName='Remark' FieldRules='@context.Entity'>
        <TextArea Placeholder="@LanguageService[$"{CurrencyConstant.Page}.{CurrencyConstant.InputRemark}"]"  AllowClear="true" @bind-Value="@context.Entity.Remark" />
    </CavFormItem>
</Form>