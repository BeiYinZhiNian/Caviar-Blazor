@namespace AntDesign.Pro.Template.Pages.Exception
@page "/exception/404"
@inject NavigationManager NavigationManager
@layout CavEmptyLayout

<Result Status="404"
        Title="404"
        SubTitle="抱歉，您访问的页面不存在。">
    <Extra>
        <Button Type="primary" OnClick="BackHome">回到首页</Button>
    </Extra>
</Result>

@{
    void BackHome()
    {
        NavigationManager.NavigateTo("/");
    }

}